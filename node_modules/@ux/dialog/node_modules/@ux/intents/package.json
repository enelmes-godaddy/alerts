  "version": "2300.2.6",
  "gitHead": "0af619e8feab29b260e1eb53c4369cdf4c44137b",
  "description": "A collection of variables for use in themes",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "files": [
    "src",
    "lib",
    "babel.config.js",
    ".babelrc.js"
  ],
  "scripts": {
    "build": "babel src -d lib && npm run build:sass && npm run build:list",
    "build:list": "node ./bin/list-intents.js",
    "build:sass": "node ./bin/build-sass.js",
    "test": "nyc --reporter=text --reporter=json-summary npm run test:unit",
    "test:unit": "mocha -r @babel/register test/*.test.js",
    "lint": "eslint '{src,test}/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rimraf ./lib ./dist ./node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gdcorp-uxp/uxcore2.git"
  },
  "publishConfig": {
    "registry": "https://gdartifactory1.jfrog.io/artifactory/api/npm/node-uxplatform-local/"
  },
  "author": "GoDaddy Operating Company, LLC",
  "license": "UNLICENSED",
  "dependencies": {
    "@ux/short-css-vars": "^2300.2.6",
    "camelcase": "^6.0.0",
    "obj-dot-props": "^1.1.0",
    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/register": "^7.7.0",
    "@ux/babel-config": "^2300.2.6",
    "assume": "^2.2.0",
    "eslint": "^8.19.0",
    "eslint-config-godaddy": "^6.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.2.2",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "rimraf": "3.0.2",
    "setup-env": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "godaddy"
    ],
    "plugins": [
      "import"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "error"
      ]
    }
  }

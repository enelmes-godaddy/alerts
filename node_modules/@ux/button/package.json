{
  "name": "@ux/button",
  "homepage": "https://github.com/gdcorp-uxp/uxcore2/tree/release-2201/packages/components/button",
  "version": "2301.3.0",
  "gitHead": "9a771b3da41ccb3446f8d1c06414d7bb905a32c2",
  "description": "Set of buttons that can be used with Components",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "build": "webpack",
  "locales": [
    "en-US"
  ],
  "files": [
    "src",
    "lib",
    "dist",
    "examples",
    "babel.config.js",
    "linaria.config.js",
    "webpack.config.js",
    "wrhs.toml",
    "empty.js",
    ".babelrc.js"
  ],
  "scripts": {
    "build": "npm run build:babel",
    "build:babel": "babel src -d lib && linaria-to-css",
    "clean": "rimraf ./lib ./dist ./node_modules",
    "cypress:approve": "npm test -- --env updateSnapshots=1",
    "cypress:open": "CYPRESS_INTERNAL_BROWSER_CONNECT_TIMEOUT=180000 CYPRESS_CRASH_REPORTS=0 cypress open --component",
    "docgen": "ux-react-docgen ./src/**/*.js -e '*/styles.js' -e '*/intents.config.js' -o ./dist/api",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "eslint '{src,rules,test,examples}/**/*.js'",
    "lint:styles": "stylelint 'src/styles.js'",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run clean && npm run build && npm run docgen",
    "pretest": "npm run build",
    "report": "nyc report --reporter=lcov",
    "storybook": "start-storybook -p 9001",
    "test": "CYPRESS_INTERNAL_BROWSER_CONNECT_TIMEOUT=180000 CYPRESS_CRASH_REPORTS=0 nyc --reporter=text --reporter=json-summary cypress run --component --headless --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gdcorp-uxp/uxcore2.git"
  },
  "keywords": [
    "Button",
    "React",
    "Component"
  ],
  "publishConfig": {
    "registry": "https://gdartifactory1.jfrog.io/artifactory/api/npm/node-uxplatform-local/"
  },
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Martijn Swaagman <mswaagman@godaddy.com>",
    "Lee Glasgow <lglasgow@godaddy.com>"
  ],
  "contributors": [
    "Arnout Kazemier <akazemier@godaddy.com>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@ux/icon": "^2301.2.1",
    "@ux/intents": "^2301.1.0",
    "@ux/text": "^2301.1.0",
    "classnames": "^2.3.1"
  },
  "peerDependencies": {
    "linaria": "2.3.1",
    "prop-types": "^15.7.2",
    "react": "17.x || 18.x",
    "react-dom": "17.x || 18.x"
  },
  "peerDependenciesMeta": {
    "linaria": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/register": "^7.7.0",
    "@cypress/react18": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@ux/babel-config": "^2301.1.0",
    "@ux/cypress-config": "^2301.1.0",
    "@ux/linaria-config": "^2301.1.0",
    "@ux/react-docgen": "^2301.1.0",
    "@ux/storybook-config": "^2301.1.0",
    "babel-eslint": "^10.0.3",
    "cypress": "^13.2.0",
    "eslint": "^8.19.0",
    "eslint-config-godaddy-react": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.19.0",
    "jsdom": "^16.5.0",
    "linaria": "2.3.1",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "3.0.2",
    "sinon": "^7.5.0",
    "stylelint": "^14.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "error"
      ]
    },
    "root": false
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "exports": {
    "./dossier/api/*": "./dist/api/src/*",
    "./package.json": "./package.json",
    "./dist/styles.css": "./dist/styles.css",
    "./styles": {
      "development": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "production": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "node": "./empty.js",
      "default": "./dist/styles.css"
    },
    ".": {
      "browser": "./lib/index.js",
      "module": "./src/index.js",
      "default": "./lib/index.js"
    },
    "./src/styles": "./src/styles.js"
  }
}
